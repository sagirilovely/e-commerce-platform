#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/15571/Desktop/e-commerce-platform/node/node_modules/.pnpm/pnpm@9.15.1/node_modules/pnpm/bin/node_modules:/mnt/c/Users/15571/Desktop/e-commerce-platform/node/node_modules/.pnpm/pnpm@9.15.1/node_modules/pnpm/node_modules:/mnt/c/Users/15571/Desktop/e-commerce-platform/node/node_modules/.pnpm/pnpm@9.15.1/node_modules:/mnt/c/Users/15571/Desktop/e-commerce-platform/node/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/15571/Desktop/e-commerce-platform/node/node_modules/.pnpm/pnpm@9.15.1/node_modules/pnpm/bin/node_modules:/mnt/c/Users/15571/Desktop/e-commerce-platform/node/node_modules/.pnpm/pnpm@9.15.1/node_modules/pnpm/node_modules:/mnt/c/Users/15571/Desktop/e-commerce-platform/node/node_modules/.pnpm/pnpm@9.15.1/node_modules:/mnt/c/Users/15571/Desktop/e-commerce-platform/node/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pnpm/bin/pnpm.cjs" "$@"
else
  exec node  "$basedir/../pnpm/bin/pnpm.cjs" "$@"
fi
