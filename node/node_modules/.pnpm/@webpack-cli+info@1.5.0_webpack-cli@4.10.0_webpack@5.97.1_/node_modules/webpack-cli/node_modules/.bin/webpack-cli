#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/15571/Desktop/e-commerce-platform/node/node_modules/.pnpm/webpack-cli@4.10.0_webpack@5.97.1/node_modules/webpack-cli/bin/node_modules:/mnt/c/Users/15571/Desktop/e-commerce-platform/node/node_modules/.pnpm/webpack-cli@4.10.0_webpack@5.97.1/node_modules/webpack-cli/node_modules:/mnt/c/Users/15571/Desktop/e-commerce-platform/node/node_modules/.pnpm/webpack-cli@4.10.0_webpack@5.97.1/node_modules:/mnt/c/Users/15571/Desktop/e-commerce-platform/node/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/15571/Desktop/e-commerce-platform/node/node_modules/.pnpm/webpack-cli@4.10.0_webpack@5.97.1/node_modules/webpack-cli/bin/node_modules:/mnt/c/Users/15571/Desktop/e-commerce-platform/node/node_modules/.pnpm/webpack-cli@4.10.0_webpack@5.97.1/node_modules/webpack-cli/node_modules:/mnt/c/Users/15571/Desktop/e-commerce-platform/node/node_modules/.pnpm/webpack-cli@4.10.0_webpack@5.97.1/node_modules:/mnt/c/Users/15571/Desktop/e-commerce-platform/node/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
