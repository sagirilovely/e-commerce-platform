#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/admin/Desktop/e-commerce platform/node/node_modules/.pnpm/ts@0.2.2_typescript@5.7.2/node_modules/ts/dist/node_modules:/mnt/c/Users/admin/Desktop/e-commerce platform/node/node_modules/.pnpm/ts@0.2.2_typescript@5.7.2/node_modules/ts/node_modules:/mnt/c/Users/admin/Desktop/e-commerce platform/node/node_modules/.pnpm/ts@0.2.2_typescript@5.7.2/node_modules:/mnt/c/Users/admin/Desktop/e-commerce platform/node/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/admin/Desktop/e-commerce platform/node/node_modules/.pnpm/ts@0.2.2_typescript@5.7.2/node_modules/ts/dist/node_modules:/mnt/c/Users/admin/Desktop/e-commerce platform/node/node_modules/.pnpm/ts@0.2.2_typescript@5.7.2/node_modules/ts/node_modules:/mnt/c/Users/admin/Desktop/e-commerce platform/node/node_modules/.pnpm/ts@0.2.2_typescript@5.7.2/node_modules:/mnt/c/Users/admin/Desktop/e-commerce platform/node/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli.js" "$@"
fi
